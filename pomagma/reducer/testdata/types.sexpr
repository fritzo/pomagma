;; UNIT
(EQUAL UNIT UNIT)
(EQUAL (UNIT x) (UNIT x))
(EQUAL (UNIT (UNIT x)) (UNIT x))
(EQUAL (UNIT TOP) TOP)
(EQUAL (UNIT BOT) BOT)
(EQUAL (UNIT I) I)
(EQUAL (UNIT K) TOP)
(EQUAL (UNIT B) TOP)
(EQUAL (UNIT C) TOP)
(EQUAL (UNIT S) TOP)
(EQUAL (UNIT lib.join) TOP)

;; BOOL
(EQUAL (BOOL x) (BOOL x))
(EQUAL (BOOL (BOOL x)) (BOOL x))
(EQUAL (BOOL TOP) TOP)
(EQUAL (BOOL BOT) BOT)
(EQUAL (BOOL K) K)
(EQUAL (BOOL (K I)) (K I))
(EQUAL (BOOL I) TOP)
(EQUAL (BOOL B) TOP)
(EQUAL (BOOL C) TOP)
(EQUAL (BOOL S) TOP)
(EQUAL (BOOL lib.join) TOP)

;; MAYBE
(EQUAL (MAYBE x) (MAYBE x))
(EQUAL (MAYBE (MAYBE x)) (MAYBE x))
(EQUAL (MAYBE TOP) TOP)
(EQUAL (MAYBE BOT) BOT)
(EQUAL (MAYBE K) K)
(EQUAL (MAYBE (K (C I TOP))) (K (C I TOP)))
(EQUAL (MAYBE (K (C I BOT))) (K (C I BOT)))
(EQUAL (MAYBE (K (C I I))) (K (C I I)))
(EQUAL (MAYBE I) TOP)
(EQUAL (MAYBE B) TOP)
(EQUAL (MAYBE C) TOP)
(EQUAL (MAYBE S) TOP)
(EQUAL (MAYBE lib.join) TOP)

;; CODE
(EQUAL (CODE x) (CODE x))
(EQUAL (CODE (CODE x)) (CODE x))
(EQUAL (CODE TOP) TOP)
(EQUAL (CODE BOT) BOT)
(EQUAL (CODE (QUOTE x)) (QUOTE x))
(EQUAL (CODE (QUOTE TOP)) (QUOTE TOP))
(EQUAL (CODE (QUOTE BOT)) (QUOTE BOT))
(EQUAL (CODE (QUOTE I)) (QUOTE I))
(EQUAL (CODE (QUOTE (QUOTE x))) (QUOTE (QUOTE x)))
(EQUAL (CODE (QQUOTE x)) (QQUOTE x))
(EQUAL (CODE (QAPP x y)) (QAPP x y))
