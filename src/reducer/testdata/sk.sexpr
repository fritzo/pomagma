;; variables
(EQUAL (QUOTE x) (QUOTE x))
(EQUAL (QUOTE x y) (QUOTE x y))
(EQUAL (QUOTE x I) (QUOTE x I))

;; TOP
(EQUAL (QUOTE TOP) (QUOTE TOP))
(EQUAL (QUOTE (TOP x) (QUOTE TOP))
(EQUAL (QUOTE (TOP x y) (QUOTE TOP))

;; BOT
(EQUAL (QUOTE BOT) (QUOTE BOT))
(EQUAL (QUOTE (BOT x) (QUOTE BOT))
(EQUAL (QUOTE (BOT x y) (QUOTE BOT))

;; I
(EQUAL (QUOTE I) (QUOTE I))
(EQUAL (QUOTE (I x)) (QUOTE x))
(EQUAL (QUOTE (I K)) (QUOTE K))
(EQUAL (QUOTE (I (B B B))) (QUOTE (B B B)))  ; xfail continuation

;; K
(EQUAL (QUOTE K) (QUOTE K))
(EQUAL (QUOTE (K x)) (QUOTE (K x)))
(EQUAL (QUOTE (K x y)) (QUOTE x))
(EQUAL (QUOTE (K x y z)) (QUOTE (x z)))

;; B
(EQUAL (QUOTE B) (QUOTE B))
(EQUAL (QUOTE (B x)) (QUOTE (B x)))
(EQUAL (QUOTE (B x y)) (QUOTE (B x y)))
(EQUAL (QUOTE (B x y z)) (QUOTE (x (y z))))
(EQUAL (QUOTE (B x y z w)) (QUOTE (x (y z) w)))
(EQUAL (QUOTE (B B)) (QUOTE (B B)))  ; xfail continuation
(EQUAL (QUOTE (B B B)) (QUOTE (B B B)))  ; xfail continuation

;; C
(EQUAL (QUOTE C) (QUOTE C))
(EQUAL (QUOTE (C x)) (QUOTE (C x)))
(EQUAL (QUOTE (C x y)) (QUOTE (C x y)))
(EQUAL (QUOTE (C x y z)) (QUOTE (x z y)))
(EQUAL (QUOTE (C x y z w)) (QUOTE (x z y w)))

;; S
(EQUAL (QUOTE S) (QUOTE S))
(EQUAL (QUOTE (S x)) (QUOTE (S x)))
(EQUAL (QUOTE (S x y)) (QUOTE (S x y)))
(EQUAL (QUOTE (S x y z)) (QUOTE (x z (y z))))
(EQUAL (QUOTE (S x y z w)) (QUOTE (x z (y z) w)))
