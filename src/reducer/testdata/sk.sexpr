;; variables
(EQUAL x x)
(EQUAL (x y) (x y))
(EQUAL (x I) (x I))

;; TOP
(EQUAL TOP TOP)
(EQUAL (TOP x) (TOP)
(EQUAL (TOP x y) (TOP)

;; BOT
(EQUAL BOT BOT)
(EQUAL (BOT x) BOT)
(EQUAL (BOT x y) BOT)

;; I
(EQUAL I I)
(EQUAL (I x) x)
(EQUAL (I K) K)
(EQUAL (I (B B B)) (B B B))

;; K
(EQUAL K K)
(EQUAL (K x) (K x))
(EQUAL (K x y) x)
(EQUAL (K x y z) (x z))

;; B
(EQUAL B B)
(EQUAL (B x) (B x))
(EQUAL (B x y) (B x y))
(EQUAL (B x y z) (x (y z)))
(EQUAL (B x y z w) (x (y z) w))
(EQUAL (B B) (B B))
(EQUAL (B B B) (B B B))

;; C
(EQUAL C C)
(EQUAL (C x) (C x))
(EQUAL (C x y) (C x y))
(EQUAL (C x y z) (x z y))
(EQUAL (C x y z w) (x z y w))

;; S
(EQUAL S S)
(EQUAL (S x) (S x))
(EQUAL (S x y) (S x y))
(EQUAL (S x y z) (x z (y z)))
(EQUAL (S x y z w) (x z (y z) w))
